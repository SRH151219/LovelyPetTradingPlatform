page{
  height: 100%;
  background: #f2f2f2;
}
.search {
  width: 100%;
  height: 90rpx;
  background: #03bbd5;
  display: flex;
  align-items: center;
  justify-content: center;
}

.search input {
  width: 80%;
  height: 60rpx;
  border-radius: 10rpx;
  background: #fff;
  padding-left: 18rpx;
  font-size: 32rpx;
}

.search text {
  font-size: 32rpx;
  padding-left: 20rpx;
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
}

.con {
  padding: 20rpx;
  background: #FFF;
  max-height: 150rpx;
  margin-bottom: 20rpx;
}

.con view:nth-of-type(1) {
  color: rgba(255, 166, 0, 0.555);
  font-size: 32rpx;
  margin-bottom: 16rpx;
}

.con view:nth-of-type(2) {
  color: #444;
  font-size: 34rpx;
  max-height: 100rpx;
  overflow: hidden;
  line-height: 50rpx;
}
